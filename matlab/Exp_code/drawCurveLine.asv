
%%稳定性——画折线图

clc
close all

x = 1:100; % x轴数据
y = load("C:\Users\Lenovo\Desktop\11-3Data\stability_2.txt"); % y轴数据

x_data = y(:,1);
y_data = y(:,2);
z_data = y(:,3);
tiledlayout(3,1)



% Top plot
ax1 = nexttile;
plot(ax1,x,x_data)
xticks([]) % 去除X轴刻度
ylabel(ax1,'X/mm')

% Bottom plot
ax2 = nexttile;
plot(ax2,x,y_data)
xticks([]) % 去除X轴刻度
ylabel(ax2,'Y/mm')

% Bottom plot
ax3 = nexttile;
plot(ax3,x,z_data)
xlabel(ax3,'Number of Samples')
ylabel(ax3,'Z/mm')
